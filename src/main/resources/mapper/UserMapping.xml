<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.summer.springvuedemo.demo.dao.UserDao">
    <select id="getUserByMessage" resultType="com.summer.springvuedemo.demo.entity.User">
        select * from users where user_name=#{username} and password=#{password} and status=1
    </select>
</mapper>